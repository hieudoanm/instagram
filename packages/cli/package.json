{
  "name": "@instagram/cli",
  "version": "0.0.1",
  "description": "Instagram CLI",
  "main": "./src/main.ts",
  "author": "hieudoanm <hieumdoan@gmail.com> (https://hieudoanm.github.io)",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "lint": "eslint --fix .",
    "format": "prettier --cache --write .",
    "build": "rimraf dist && tsc",
    "install-chrome": "puppeteer browsers install chrome",
    "install-cli": "pnpm link --global .",
    "preinstall-cli": "pnpm run build",
    "download": "rimraf temp && mkdir temp && pnpm ts-node ./src/main.ts"
  },
  "dependencies": {
    "@instagram/sdk": "workspace:*",
    "@oclif/core": "4.2.7",
    "figlet": "1.8.0",
    "puppeteer": "24.2.1"
  },
  "devDependencies": {
    "@eslint/js": "9.20.0",
    "@types/figlet": "1.7.0",
    "@types/node": "22.13.4",
    "eslint": "9.20.1",
    "globals": "15.15.0",
    "oclif": "4.17.30",
    "prettier": "3.5.1",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.24.1"
  },
  "keywords": [
    "instagram",
    "oclif",
    "cli"
  ],
  "bin": {
    "instagram-web-cli": "./bin/run.js"
  },
  "oclif": {
    "bin": "instagram-web-cli",
    "commands": "./dist/commands",
    "dirname": "cli",
    "topicSeparator": " "
  }
}
